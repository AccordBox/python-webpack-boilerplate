{
  "name": "python-webpack-boilerplate",
  "version": "1.0.4",
  "description": "Webpack boilerplate for Django & Flask",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "start": "webpack serve --config webpack/webpack.config.dev.js",
    "watch": "webpack --watch --config webpack/webpack.config.watch.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AccordBox/python-webpack-boilerplate"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend"
  ],
  "author": "Michael Yin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AccordBox/python-webpack-boilerplate/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.26.7",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.20.1",
    "eslint-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-loader": "^8.0.0",
    "postcss-preset-env": "^10.0.3",
{% if cookiecutter.style_solution == 'scss' %}
    "sass": "1.77.6",
    "sass-loader": "^16.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
{% elif cookiecutter.style_solution == 'tailwind' %}
    "tailwindcss": "^4.0.3",
    "@tailwindcss/postcss": "^4.0.3",
{% elif cookiecutter.style_solution == 'bootstrap' %}
    "sass": "1.77.6",
    "sass-loader": "^16.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "bootstrap": "^5.3.3",
{% endif %}
    "style-loader": "^4.0.0",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-webpack-plugin": "^5.0.1",
    "webpack": "^5.97.1",
    "webpack-assets-manifest": "^5.2.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "core-js": "^3.37.0"
  }
}
